{
  "folder": {
    "root": "./",
    "names": ["temp", "processed"]
  },
  "sources": {
    "channels": "./data/channels.tracked.custom.json",
    "downloader": "./data/downloader.args.custom.json",
    "linkIdentifier": "./data/archive.links.txt",
    "titles": "./data/archive.names.txt"
  },
  "scripts": [
    {
      "id": 0,
      "path": "./scripts/generateArchiveNamesFromSrc.py",
      "args": [
        "D:\\Music\\",
        "./data/archive.names.txt"
      ]
    },
    {
      "id": 1,
      "path": "./scripts/jsonNameCreator.py",
      "args": [
        "./processed",
        "./temp/file.json"
      ]
    }
  ],
  "executionEngine": {
    "playlist": {
      "execute?": true,
      "default": "https://www.youtube.com/playlist?list=PLYo4GDM-6t3PgIxb-VyCK6ahNLomxqbJA",
      "outputIn": "./processed",
      "runSafeAfterFail": true,
      "shellLocation": "./temp/",
      "shellName": "playlist.Executive.sh",
      "shellNameSafe": "playlist.Executive.Safe.sh",
      "renamingFilesUI": "todo",
      "shouldUpdateNameArchive": true,
      "outputMove": "D:\\Music\\"
    },
    "channel": {
      "execute?": true,
      "outputIn": "./processed",
      "runSafeAfterFail": true,
      "launchPlaylist": {
        "WARN": "Please rename the files WITHOUT DELETING ids wrapped in {} - \n",
        "attentionGrabOrCreateLocalWebpage": "start https://www.google.com/",
        "compareFilesAndDelete": true,
        "renameBeforeLaunch": true,
        "launchMode": "shell",
        "output": "generatePlaylist.sh",
        "outputIn": "temp"
      },
      "outputMove": "D:\\Music\\"
    },
    "updateArchiveNamesWithGenerator": {
      "execute?": true,
      "locationOfFiles": "D:\\Music\\",
      "provideArchiveFileFromData": true
    }
  }
}
