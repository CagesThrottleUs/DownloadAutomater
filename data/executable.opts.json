{
  "directories": ["temp", "processed"],
  "data": {
    "channels": "./data/channels.tracked.json",
    "downloader": "./data/downloader.args.json",
    "archive": {
      "links": "./data/archive.links.txt",
      "names": "./data/archive.names.txt"
    },
    "generators": {
      "names": "./scripts/generateArchiveNamesFromSrc.py"
    }
  },
  "executionEngine": {
    "playlist": {
      "execute?": true,
      "default": "https://www.youtube.com/playlist?list=PLYo4GDM-6t3PgIxb-VyCK6ahNLomxqbJA",
      "outputIn": "processed",
      "runSafeAfterFail": true,
      "outputMove": "D:\\Music\\"
    },
    "channel": {
      "execute?": true,
      "outputIn": "processed",
      "runSafeAfterFail": true,
      "launchPlaylist": {
        "WARN": "Please rename the files WITHOUT DELETING ids wrapped in {} - \n",
        "attentionGrabOrCreateLocalWebpage": "start https://www.google.com/",
        "compareFilesAndDelete": true,
        "renameBeforeLaunch": true,
        "launchMode": "shell",
        "output": "generatePlaylist.sh",
        "outputIn": "temp"
      },
      "outputMove": "D:\\Music\\"
    },
    "updateArchiveNamesWithGenerator": {
      "execute?": true,
      "locationOfFiles": "D:\\Music\\",
      "provideArchiveFileFromData": true
    },
    "remixArchiveLinksDirectly": {
      "execute?": true,
      "provideArchiveFileFromData": true
    }
  }
}
